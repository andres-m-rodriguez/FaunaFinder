{
  "compilerOptions": {
    "target": "ES2020",
    "module": "None",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": ".",
    "rootDir": ".",
    "declaration": false,
    "sourceMap": false,
    "noEmit": false,
    "removeComments": false
  },
  "include": ["leafletInterop.ts"],
  "exclude": ["node_modules", "types.ts"]
}
